def printElements(mat, n) :
 
    for i in range(n) :
        if i % 2 == 0 :
            for j in range(0, n, 2) :
                print(mat[i][j],end = " ")
        else :
            for j in range(1, n, 2) :
                print(mat[i][j],end =" ")
             
     
if __name__ == "__main__" :
 
    n = 3
    mat = [ [ 1, 5, 3],
            [ 2, 4, 7],
            [ 9, 8, 6] ]
 
    printElements(mat , n)
